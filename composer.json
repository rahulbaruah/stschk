{
    "name": "rahxcr/laravel-stschk",
    "description": "System library to check status",
    "keywords": [
        "rahxcr",
        "laravel-stschk"
    ],
    "homepage": "https://github.com/rahxcr/laravel-stschk",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Laravel",
            "email": "info@laravel.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Rahxcr\\LaravelStschk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rahxcr\\LaravelStschk\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rahxcr\\LaravelStschk\\LaravelStschkServiceProvider"
            ],
            "aliases": {
                "LaravelStschk": "Rahxcr\\LaravelStschk\\LaravelStschkFacade"
            }
        }
    }
}
